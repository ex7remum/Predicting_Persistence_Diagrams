{
    "arch": {
        "type": "ImageSet2Set",
        "args": {
            "n_out_max": 32,
            "d_in": 2,
            "d_out": 2,
            "d_hidden": 128,
            "d_mlp": 256,
            "n_heads": 8, 
            "n_encoder_layers": 4,
            "n_decoder_layers": 4
        }
    },
    "data": {
      "train": {
          "batch_size": 128,
          "num_workers": 2,
          "dataset": {
              "type": "PDMnist",
              "args": {
                  "data_dir": "RAW/MNIST",
                  "pd_dir": "PD/direction/10/MNIST_pds.pkl",
                  "train": true,
                  "leave": 32
              }
          }
      },
      "test": {
          "batch_size": 128,
          "num_workers": 2,
          "dataset": {
              "type": "PDMnist",
              "args": {
                  "data_dir": "RAW/MNIST",
                  "pd_dir": "PD/direction/10/MNIST_pds_test.pkl",
                  "train": false,
                  "leave": 32
              }
          }
      }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 5e-5
        }
    },
    "lr_scheduler": {
        "type": "LinearLR",
        "args": {
            "start_factor": 0.01,
            "total_iters": 1000
        }
    },
    "loss": {
        "type": "PersistenceWeightedSlicedWassersteinLoss",
        "args": {
            "q": 1, 
            "reduce": "sum",
            "random_seed": 0
        }
    },
    "trainer": {
        "n_epochs": 150,
        "wandb_project": "mnist_pds",
        "run_name": "mnist_10_filt_fixed_full",
        "grad_norm_clip": 10
    },
    "collator": {
       "type": "collate_fn"
    },
    "pimgr": {
        "bandwidth": 0.05,
        "im_range": [0.0, 0.9960784316062927, 0.0, 0.7381034392506136]
    }
}