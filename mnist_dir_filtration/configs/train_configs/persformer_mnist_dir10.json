{
    "arch": {
        "type": "CustomPersformer",
        "args": {
            "n_in": 2, 
            "embed_dim": 128, 
            "fc_dim": 256, 
            "num_heads": 8, 
            "num_layers": 4, 
            "n_out_enc": 10
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 5e-5
        }
    },
    "lr_scheduler": {
        "type": "LinearLR",
        "args": {
            "start_factor": 0.2,
            "total_iters": 250
        }
    },
    "trainer": {
        "n_epochs": 100,
        "wandb_project": "mnist_pds",
        "run_name": "mnist_10_filt_persformer",
        "grad_norm_clip": 10
    }
}