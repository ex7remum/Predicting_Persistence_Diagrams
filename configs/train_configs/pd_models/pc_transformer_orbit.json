{
    "arch": {
        "type": "Set2Set",
        "args": {
            "n_out_max": 32,
            "d_in": 2,
            "d_out": 2,
            "d_hidden": 128,
            "d_mlp": 128
        }
    },
    "data": {
      "train": {
          "batch_size": 64,
          "num_workers": 2,
          "dataset": {
              "type": "OrbitDataset",
              "args": {
                  "data_dir": "RAW/Orbit10k/orbit10000k_train.pkl",
                  "pd_dir": "PD/rips/10000/500/Orbit_pds.pkl",
                  "leave": 32
              }
          }
      },
      "test": {
          "batch_size": 64,
          "num_workers": 2,
          "dataset": {
              "type": "OrbitDataset",
              "args": {
                  "data_dir": "RAW/Orbit10k/orbit10000k_test.pkl",
                  "pd_dir": "PD/rips/10000/500/Orbit_pds_test.pkl",
                  "leave": 32
              }
          }
      }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 3e-4
        }
    },
    "lr_scheduler": {
        "type": "LinearLR",
        "args": {
            "start_factor": 0.001,
            "total_iters": 1000
        }
    },
    "loss": {
        "type": "PersistenceWeightedSlicedWassersteinLoss",
        "args": {
            "q": 1,
            "reduce": "sum",
            "random_seed": 0
        }
    },
    "trainer": {
        "n_epochs": 350,
        "wandb_project": "orbit_pds",
        "run_name": "set2set_fixed_10k_500",
        "grad_norm_clip": 10,
        "device": "cuda",
        "n_runs": 3
    },
    "pimgr": {
        "bandwidth": 0.05,
        "im_range": [0.0, 0.9960784316062927, 0.0, 0.7381034392506136]
    }
}