{
    "arch": {
        "type": "CustomPersformer",
        "args": {
            "n_in": 2,
            "embed_dim": 128,
            "fc_dim": 128,
            "num_heads": 8,
            "num_layers": 5,
            "n_out_enc": 5,
            "reduction": "attention",
            "dropout": 0.0
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 1e-3,
            "weight_decay": 1e-4
        }
    },
    "lr_scheduler": {
        "type": "LinearLR",
        "args": {
            "start_factor": 0.2,
            "total_iters": 250
        }
    },
    "trainer": {
        "n_epochs": 200,
        "wandb_project": "orbit_pds",
        "run_name": "orbit_rips_10k_500_persformer",
        "grad_norm_clip": 10
    }
}